(jbuild_version 1)

(library
 ((name cohttp)
  (public_name cohttp)
  (synopsis "Co-operative Client/Server HTTP library.")
  (preprocess (pps (ppx_fields_conv ppx_sexp_conv)))
  (libraries (re.emacs
              stringext
              uri
              fieldslib
              sexplib
              bytes
              base64))))



(ocamllex (accept_lexer))
(ocamlyacc (accept_parser))